    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --color",
    "fix": "eslint . --ext .js,.ts,.jsx,.tsx --color --fix",
    "lint:watch": "npm run lint -- --watch",
    "jest": "jest",
    "test": "jest && npm run lint",
    "test:integration": "jest --runTestsByPath test/integration/index.test.ts --detectOpenHandles --forceExit",
    "test:CI": "npm run test && npm run test:integration",
    "test:cover": "npm run test -- --coverage ",
    "test:send-cover": "jest --coverage --coverageReporters=text-lcov | coveralls"
