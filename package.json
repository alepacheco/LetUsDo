{
  "name": "we-do",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
    "dev": "npm run start",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "fix": "esw webpack.config.* src tools --color --fix",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run test && npm run clean-dist",
    "build-run": "babel-node tools/build.js && npm run open:dist",
    "build": "babel-node tools/build.js",
    "buildd": "npm run remove-dist && babel-node tools/build.js",
    "test": "npm run lint && jest && npm run build",
    "test:CI": "npm run test && npm run test:send-cover",
    "test:cover": "npm run test -- --coverage ",
    "test:send-cover": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watchAll",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Alejandro Pacheco",
  "license": "Unlicenced",
  "dependencies": {
    "@hot-loader/react-dom": "16.9.0",
    "axios": "0.19.0",
    "bootstrap": "4.3.1",
    "connected-react-router": "6.5.2",
    "enzyme-to-json": "3.4.0",
    "object-assign": "4.1.1",
    "react": "16.9.0",
    "react-bootstrap": "1.0.0-beta.12",
    "react-dom": "16.9.0",
    "react-ga": "2.6.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "react-stripe-elements": "5.0.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.6.0",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/polyfill": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@now/node": "1.0.0",
    "@types/dotenv-webpack": "1.7.0",
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.2",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.5",
    "@types/react-stripe-elements": "1.3.2",
    "@types/stripe": "6.31.28",
    "acorn": "7.0.0",
    "ajv": "6.10.2",
    "autoprefixer": "9.6.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.7",
    "chalk": "2.4.2",
    "concurrently": "4.1.2",
    "connect-history-api-fallback": "1.6.0",
    "coveralls": "3.0.6",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "dotenv": "8.1.0",
    "dotenv-webpack": "1.7.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "eslint-watch": "6.0.0",
    "express": "4.17.1",
    "file-loader": "4.2.0",
    "hard-source-webpack-plugin": "0.13.1",
    "history": "4.9.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.8.0",
    "mockdate": "2.0.5",
    "node-sass": "4.12.0",
    "opn-cli": "5.0.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-hot-loader": "4.12.12",
    "react-test-renderer": "16.9.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "replace": "1.1.1",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "stripe": "7.9.0",
    "style-loader": "1.0.0",
    "svg-react-loader": "0.4.6",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.6.2",
    "url-loader": "2.1.0",
    "webpack": "4.39.3",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-dev-middleware": "3.7.1",
    "webpack-hot-middleware": "2.25.0",
    "webpack-messages": "2.0.4"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(scss)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "api/**/**"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "ie": 10
              },
              "forceAllTransforms": true
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-dynamic-import",
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "standard": {
      "env": [
        "mocha"
      ]
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true,
      "mocha": true,
      "jest/globals": true
    },
    "globals": {}
  }
}
