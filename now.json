{
  "name": "WeDo",
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    {
      "src": "/.well-known/(.*)",
      "dest": "/static/.well-known/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    },
    {
      "src": "/api/**",
      "use": "@now/node"
    }
  ],
  "build": {
    "env": {
      "STRIPE_SERVER": "@stripe-server-live",
      "STRIPE_FRONT": "@stripe-front-live"
    }
  },
  "env": {
    "IS_NOW": "true",
    "STRIPE_SERVER": "@stripe-server-live",
    "STRIPE_FRONT": "@stripe-front-live"
  }
}
