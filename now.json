{
  "routes": [
    {
      "src": "/static/fonts/(?<id>[^/]*)",
      "dest": "/dist/$id"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/(?:^|(?<= ))([^\\. ]+)(?:\n|(?= )|$)/g",
      "dest": "/dist/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    {
      "src": "dist/*/**",
      "use": "@now/static"
    },
    {
      "src": "dist/**",
      "use": "@now/static"
    }
  ]
}